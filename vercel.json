{
  "version": 2,
  "name": "automatizafix",
  "builds": [
    {
      "src": "api/enviar-consulta.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/enviar-consulta",
      "dest": "/api/enviar-consulta.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/enviar-consulta.py": {
      "runtime": "python3.9"
    }
  }
}